{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.2",
    "marked": "^4.2.12",
    "modern-css-reset": "^1.4.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "sanitize-html": "^2.3.2",
    "typeface-lato": "0.0.75",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap --debug && npm run postbuild:_redirects && npm run postbuild:_headers && npm run postbuild:netlify && npm run postbuild:info",
    "postbuild:_redirects": "envsubst < public/_redirects > build/_redirects",
    "postbuild:_headers": "envsubst < public/_headers > build/_headers",
    "postbuild:netlify": "envsubst < template.netlify.toml > netlify.toml",
    "postbuild:info": "cat build/_redirects && cat build/_headers && cat netlify.toml",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "reactSnap": {
    "include": [
      "/projects/href"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/sanitize-html": "^2.8.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-proxy-middleware": "^2.0.6",
    "react-snap": "^1.23.0",
    "sass": "^1.59.3",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^19.0.0",
    "typescript": "^4.9.5"
  }
}
